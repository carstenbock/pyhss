                                <!--Optional:-->
                                <cp:conditions>
                                    <!--You have a CHOICE of the next 4 items at this level-->
                                    <!--Optional:-->
                                    <cp:identity>
                                        <!--You have a CHOICE of the next 3 items at this level-->
                                        <cp:one id="https://www.test.org/pectore/talia">
                                            <!--You may enter ANY elements at this point-->
